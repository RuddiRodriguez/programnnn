function [alfa] = correction_factor_time(kappa,modo,radio,epsilon,b,fps)

if ((modo/radio)<1e-6)
    coeficiente=2
end

if ((modo/radio)>1e-6)
    coeficiente=8
end

q=modo/radio
relaxation_time=(inv(kappa*(q^3)/(4*0.001)))+inv((epsilon.*(q.^2))./(coeficiente.*b))
integration_time=inv(fps)

cociente=relaxation_time/integration_time;

fcorr=2*(cociente^2)*(exp(-inv(cociente))-(1-(inv(cociente))))